      "content": "use leptos::*;\nuse leptos_meta::Stylesheet;\n\n#[component]\npub fn DemoUniverseRotating() -> impl IntoView {\n    view! {\n        <Stylesheet id=\"universe-rotating\" href=\"/components/universe-rotating.css\" />\n\n        <div class=\"p-20 w-full max-w-2xl border mainDivUniverse border-sky-500\">\n\n            <div class=\"universe\">\n                <div class=\"orbit moon\"></div>\n                <div class=\"orbit mercury\"></div>\n                <div class=\"orbit venus\"></div>\n                <div class=\"orbit sun\"></div>\n                <div class=\"orbit mars\"></div>\n                <div class=\"orbit jupiter\"></div>\n                <div class=\"orbit saturn\"></div>\n                <div class=\"orbit stars\">\n                    <div class=\"star\" style=\"--j:0; --y:.1\"></div>\n                    <div class=\"star\" style=\"--j:1; --y:.5\"></div>\n                    <div class=\"star\" style=\"--j:2; --y:.2\"></div>\n                    <div class=\"star\" style=\"--j:3; --y:.1\"></div>\n                    <div class=\"star\" style=\"--j:4; --y:.6\"></div>\n                    <div class=\"star\" style=\"--j:5; --y:.2\"></div>\n                    <div class=\"star\" style=\"--j:6; --y:.9\"></div>\n                    <div class=\"star\" style=\"--j:7; --y:.4\"></div>\n                    <div class=\"star\" style=\"--j:8; --y:.3\"></div>\n                    <div class=\"star\" style=\"--j:9; --y:.5\"></div>\n                    <div class=\"star\" style=\"--j:10; --y:.3\"></div>\n                    <div class=\"star\" style=\"--j:11; --y:.4\"></div>\n                    <div class=\"star\" style=\"--j:12; --y:.7\"></div>\n                    <div class=\"star\" style=\"--j:13; --y:.8\"></div>\n                    <div class=\"star\" style=\"--j:14; --y:.1\"></div>\n                    <div class=\"star\" style=\"--j:15; --y:.2\"></div>\n                    <div class=\"star\" style=\"--j:16; --y:.9\"></div>\n    