      "content": "use leptos::*;\nuse leptos_meta::Stylesheet;\n\n// Source: https://codepen.io/tdoughty/pen/bGGZWqg (more trees and birds)\n\n#[component]\npub fn DemoCssPillMountain() -> impl IntoView {\n    view! {\n        <Stylesheet id=\"css-pill-mountain\" href=\"/components/css-pill-mountain.css\" />\n\n        <div class=\"flex justify-center items-center mx-auto w-full max-w-2xl\">\n\n            <div class=\"mx-auto w-full max-w-2xl\">\n                <div class=\"containerPillMountain\">\n                    <div class=\"birds front\">\n                        <div class=\"bird b1\">\n                            <div class=\"body\">\n                                <div class=\"wing1\">\n                                    <div class=\"wing2\">\n                                        <div class=\"wing3\"></div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n\n                        <div class=\"bird b12\">\n                            <div class=\"body\">\n                                <div class=\"wing1\">\n                                    <div class=\"wing2\">\n                                        <div class=\"wing3\"></div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"birds back\">\n                        <div class=\"bird b1\">\n                            <div class=\"body\">\n                                <div class=\"wing1\">\n                                    <div class=\"wing2\">\n                                        <div class=\"wing3\"></div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n\n                        <div class=\"bird b12\">\n                            <div class=\"body\">\n                                <div class=\"wing1\">\n                                    <div class=\"wing2\">\n                                        <div class=\"wing3\"></div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"cloud big\">\n                        <div class=\"circle c0\"></div>\n\n                        <div class=\"circle c8\"></div>\n                    </div>\n                    <div class=\"cloud small\">\n                        <div class=\"circle c0\"></div>\n\n                        <div class=\"circle c8\"></div>\n                    </div>\n                    <div class=\"mountain\">\n                        <div class=\"backdrop\"></div>\n                        <div class=\"zig zag0\"></div>\n\n                        <div class=\"zig zag4\"></div>\n                        <div class=\"top\"></div>\n                        <div class=\"mid\"></div>\n                        <div class=\"bot\"></div>\n                        <div class=\"base\"></div>\n                    </div>\n                    <div class=\"range\">\n                        <div class=\"r1\"></div>\n\n                        <div class=\"r7\"></div>\n                    </div>\n                    <div class=\"tree treeBack tree1\">\n                        <div class=\"top\"></div>\n                        <div class=\"mid\"></div>\n                        <div class=\"bot\"></div>\n                        <div class=\"base\"></div>\n                    </div>\n\n                    <div class=\"tree treeBack tree8\">\n                        <div class=\"top\"></div>\n                        <div class=\"mid\"></div>\n                        <div class=\"bot\"></div>\n                        <div class=\"base\"></div>\n                    </div>\n                    <div class=\"tower\">\n                        <div class=\"shadow\"></div>\n                        <div class=\"flagPole\"></div>\n                        <div class=\"roof1\"></div>\n                        <div class=\"roof2\"></div>\n                        <div class=\"wall\">\n                            <div class=\"w1\"></div>\n\n                            <div class=\"w5\"></div>\n                        </div>\n                        <div class=\"legs\">\n                            <div class=\"left\"></div>\n                            <div class=\"right\"></div>\n                            <div class=\"support1\">\n                                <div class=\"criss\"></div>\n                                <div class=\"cross\"></div>\n                                <div class=\"flat\"></div>\n                            </div>\n                            <div class=\"support2\">\n                                <div class=\"criss\"></div>\n                                <div class=\"cross\"></div>\n                                <div class=\"flat\"></div>\n                            </div>\n                        </div>\n                        <div class=\"railing\">\n                            <div class=\"top\"></div>\n                            <div class=\"bot1\"></div>\n                            <div class=\"bot2\"></div>\n                            <div class=\"r1\"></div>\n\n                            <div class=\"r9\"></div>\n                        </div>\n                    </div>\n                    <div class=\"tree treeMid tree1\">\n                        <div class=\"top\"></div>\n                        <div class=\"mid\"></div>\n                        <div class=\"bot\"></div>\n                        <div class=\"base\"></div>\n                    </div>\n\n                    <div class=\"tree treeMid tree5\">\n                        <div class=\"top\"></div>\n                        <div class=\"mid\"></div>\n                        <div class=\"bot\"></div>\n                        <div class=\"base\"></div>\n                    </div>\n                    <div class=\"tree treeFront tree1\">\n                        <div class=\"top\"></div>\n                        <div class=\"mid\"></div>\n                        <div class=\"bot\"></div>\n                        <div class=\"base\"></div>\n                    </div>\n\n                    <div class=\"tree treeFront tree4\">\n                        <div class=\"top\"></div>\n                        <div class=\"mid\"></div>\n                        <div class=\"bot\"></div>\n                        <div class=\"base\"></div>\n                    </div>\n                </div>\n            </div>\n\n        </div>\n    }\n}\n"
    }
